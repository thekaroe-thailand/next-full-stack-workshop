generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Apartment {
  id        String   @id @default(cuid())
  name      String
  address   String
  phone     String
  email     String?
  lineId    String?
  taxCode   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}

model RoomType {
  id        String   @id @default(cuid())
  name      String
  price     Int
  remark    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
